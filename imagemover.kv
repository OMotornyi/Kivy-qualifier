<ImageFrame>:
    img: pic
    scat: logo_py
    PydisLogo:
        id: logo_py
        size: pic.size
        size_hint: None, None
        on_kv_post:
            print("on_kv_post fired")
            self.center = self.parent.center
        Image:
            id: pic
            #source: root.source
            #center: self.parent.center
            source: 'data/python_discord_logo.png'
            size_hint_x: None
            size_hint_y: None
            allow_stretch: True
            keep_ratio: False
